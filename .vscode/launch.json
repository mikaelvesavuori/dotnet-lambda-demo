{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Lambda Test Tool",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${env:HOME}/.dotnet/tools/dotnet-lambda-test-tool-6.0",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
